@using Haushaltsbuch.UI.Web.Extensions
@model EventsIndexViewModel
@{
    ViewData[index: "Title"] = "Index";
}

<h1>Index</h1>

<table class="table table-striped">
    <thead>
        <tr>
            <td>Event</td>
        </tr>
    </thead>
    <tbody>
        @foreach (string eventData in Model.Events)
        {
            <tr>
                <td>
                    <pre>@eventData.FormatJson()</pre>
                </td>
            </tr>
        }
    </tbody>
</table>
